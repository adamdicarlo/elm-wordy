{
  "private": true,
  "type": "module",
  "name": "elm-wordy",
  "version": "1.0.0",
  "description": "\"Worder\"-like game written in Elm",
  "repository": "https://github.com/adamdicarlo/elm-wordy.git",
  "author": "Adam DiCarlo <adam.dicarlo@gmail.com>",
  "license": "BSD-3-Clause",
  "scripts": {
    "postinstall": "elm-tooling install",
    "generate-dictionary": "mkdir build -p; scripts/generate-dictionary-json.js",
    "prestart": "rm -rf build; mkdir build",
    "start": "run-pty run-pty-dev.json",
    "prebuild": "yarn run generate-dictionary",
    "build": "elm-watch make --optimize"
  },
  "dependencies": {},
  "devDependencies": {
    "elm-test": "0.19.1-revision9",
    "elm-tooling": "^1.8.0",
    "elm-watch": "^1.1.2",
    "nodemon": "^2.0.22",
    "run-pty": "^4.0.3",
    "split2": "^4.1.0",
    "st": "^3.0.0",
    "through2": "^4.0.2",
    "through2-filter": "^3.0.0",
    "through2-map": "^3.0.0"
  }
}
